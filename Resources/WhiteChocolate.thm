  Font:
    Colour: '#000000'
    Padding:
      Horizontal: 3
      Vertical: 0
    Font:
      Size: 9
      Family: Courier New
  Background: [ '#FFFFFF' ]
Ruler:
  Size: 40
  Background: [ '#7C4700', '#622A0F' ]
  Lines:
    Zero:
      NumberVisible: true
      Size: 
        Vertical: 40
        Horizontal: 20      
    Colour: '#FFFFFF'
