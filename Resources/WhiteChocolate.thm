      - Interval: 20
        Colour: '#FFFFFF'
        Size: 
          Vertical: 15
          Horizontal: 15
      - Interval: 100
        Colour: '#FFFFFF'
        Size:
          Vertical: 40
          Horizontal: 20
  Numbers: 
    Font:
      Size: 9
      Family: Courier New
    Colour: '#FFFFFF'
    Padding: 
      Horizontal: 3
      Vertical: 1
    Display: 100
  Border:
    Colour: '#FFFFFF'
    Spacing: 15